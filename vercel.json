{
    "version": 2,
    "builds": [
        { "src": "api/index.php", "use": "vercel-php@0.5.2" }
        
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "tmp/$1"
        },
        {
            "src": "/(images)/(.*)",
            "dest": "public/images/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
